@model DagoWebPortfolio.Models.PicturesViewModel


<p>
    @Html.HiddenFor(model => Model.ID)


    <div class="form-group">
        <div class="control-label col-md-2">
            @Html.DisplayNameFor(model => Model.Url)
        </div>
        <div class="col-md-10">
            @Html.DisplayFor(model => Model.Url, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
    
    <div class="form-group">
        <div class="control-label col-md-2">
            @Html.DisplayNameFor(model => Model.Description)
        </div>
        <div class="col-md-10">
            @Html.DisplayFor(model => Model.Description, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
</p>